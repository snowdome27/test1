package com.internousdev.stylepet.dto;

public class PagenationDTO {
	
	private int totalPageSize;
	
	private int currentPageNo;
	
	private int totalRecordNo;
	
	private int startRecordSize;
	
	private int startRecordNo;
	
	private int endRecordNo;
	
	private boolean hasNextPage;
	
	private int nextPageNo;
	
	private int previosPageNo;
	
	private boolean hasPreviousPage;
	
	
	
	
	
	

	public int getStartRecordNo() {
		return startRecordNo;
	}

	public void setStartRecordNo(int startRecordNo) {
		this.startRecordNo = startRecordNo;
	}

	public int getTotalPageSize() {
		return totalPageSize;
	}

	public void setTotalPageSize(int totalPageSize) {
		this.totalPageSize = totalPageSize;
	}

	public int getCurrentPageNo() {
		return currentPageNo;
	}

	public void setCurrentPageNo(int currentPageNo) {
		this.currentPageNo = currentPageNo;
	}

	public int getTotalRecordNo() {
		return totalRecordNo;
	}

	public void setTotalRecordNo(int totalRecordNo) {
		this.totalRecordNo = totalRecordNo;
	}

	public int getStartRecordSize() {
		return startRecordSize;
	}

	public void setStartRecordSize(int startRecordSize) {
		this.startRecordSize = startRecordSize;
	}

	public int getEndRecordNo() {
		return endRecordNo;
	}

	public void setEndRecordNo(int endRecordNo) {
		this.endRecordNo = endRecordNo;
	}

	public boolean isHasNextPage() {
		return hasNextPage;
	}

	public void setHasNextPage(boolean hasNextPage) {
		this.hasNextPage = hasNextPage;
	}

	public int getNextPageNo() {
		return nextPageNo;
	}

	public void setNextPageNo(int nextPageNo) {
		this.nextPageNo = nextPageNo;
	}

	public int getPreviosPageNo() {
		return previosPageNo;
	}

	public void setPreviosPageNo(int previosPageNo) {
		this.previosPageNo = previosPageNo;
	}

	public boolean isHasPreviousPage() {
		return hasPreviousPage;
	}

	public void setHasPreviousPage(boolean hasPreviousPage) {
		this.hasPreviousPage = hasPreviousPage;
	}
	
	
	
	
	
	
	

}
